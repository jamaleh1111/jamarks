<h1>New Bookmark</h1>
<div class="col-md-6 col-md-offset-3">
  <%= form_for [@topic, @bookmark] do |f| %>
    <div class="form-group">
      <%= f.text_field :url, class: 'form-control', placeholder: "Enter url" %>
    </div>
    <div class="form-group">
      <%= f.submit "Save", class: 'btn btn-success' %>
    </div>
  </div>
<% end %>
